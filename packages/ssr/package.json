{
  "name": "@waika_project/ssr",
  "version": "1.0.0",
  "description": "SSR Server",
  "scripts": {
    "build": "npm run wipedist && ts-node ./scripts/esbuild.prod.ts",
    "dev": "ts-node ./ssrDev.ts",
    "preview": "npm run build && node ./dist/server.js",
    "format": "prettier --write .",
    "lint": "eslint .",
