    "test": "jest .",
    "typecheck": "tsc -p ./tsconfig.json",
    "wipedist": "rimraf dist/* --glob"
  },
  "dependencies": {
    "@waika_project/server": "1.0.0",
    "@waika_project/utils": "1.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.12.2",
    "esbuild": "^0.24.0",
    "ts-node": "^10.9.2",
    "vite": "^5.4.8"
  },
  "license": "MIT"
}
